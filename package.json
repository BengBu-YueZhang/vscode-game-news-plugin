{
	"name": "game-news",
	"displayName": "gamenews",
	"description": "surreptitiously browsing game news during work hours (gamespot, ign, gamersky)",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.游民星空",
		"onCommand:extension.IGN",
		"onCommand:extension.GameSpot",
		"onCommand:extension.GameNews"
	],
	"main": "./out/index.js",
	"contributes": {
		"configuration": {
			"title": "GameNews Configuration",
			"properties": {
				"gamenews.range": {
					"type": "number",
					"default": 1,
					"description": "过去几小时内的游戏新闻(最大可以设置3个小时前)"
				}
			}
		},
		"commands": [
			{
				"command": "extension.游民星空",
				"title": "游民星空",
				"category": "Game"
			},
			{
				"command": "extension.IGN",
				"title": "IGN",
				"category": "Game"
			},
			{
				"command": "extension.GameSpot",
				"title": "GameSpot",
				"category": "Game"
			},
			{
				"command": "extension.GameNews",
				"title": "GameNews",
				"category": "Game"
			}
		],
		"keybindings": [
			{
				"command": "extension.GameNews",
				"key": "ctrl+g+n",
				"mac": "cmd+g+n"
			}
		],
		"menus": {}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"ts-loader": "^6.2.1",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10",
		"webpack-merge": "^4.2.2"
	}
}
